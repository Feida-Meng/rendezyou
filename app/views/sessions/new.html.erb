<div class="yield">
<div class="login_window">
<%= form_tag sessions_path, {:class => "login_form"} do %>
  <div class="field">
  <%= label_tag :email %>
  <%= text_field_tag :email, params[:email] %>
</div>

<div class="field">
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</div>

<div class="submit">
  <%= submit_tag "Log in" %>
</div>

<% end %>
</div>
</div>
