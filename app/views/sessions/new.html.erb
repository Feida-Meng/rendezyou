<div class="yield">
<div class="login_window">
<%= form_tag sessions_path, {:class => "login_form"} do %>
  <%= label_tag :email %>
  <%= text_field_tag :email, params[:email] %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Log in" %>

<% end %>
</div>
</div>
